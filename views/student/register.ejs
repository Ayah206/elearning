<!DOCTYPE html>
<html ng-app="eApp">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>E-Learning | Sign up</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/bower_components/font-awesome/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="/bower_components/Ionicons/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/dist/css/AdminLTE.min.css">
  <!-- iCheck -->
  <link rel="stylesheet" href="/plugins/iCheck/square/blue.css">

  <link rel="stylesheet" href="/animate.css">
  <link rel="stylesheet" href="/custom.css">


  <link rel="stylesheet" href="/lib/dist/ngToast-animations.css">
  <link rel="stylesheet" href="/lib/dist/ngToast.css">

  <link href="/lib/angular-csp.css" rel="stylesheet">
   
   <style type="text/css">
    [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
      display: none !important;
    }
    .formError{
      color: red;
    }
  
  </style>

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body ng-cloak class="hold-transition ng-cloak register-page" ng-controller="registerCtrl">
<toast></toast>
<div class="register-box">
  <div class="register-logo">
    <a href="/studentzz"><b>SICS</a>
  </div>
   <div class="nav-tabs-custom">
    <ul class="nav nav-tabs">
      <li class="active"><a href="" data-toggle="tab">Student</a></li>
      <li><a href="/instructor/signup">Instructor</a></li>
    </ul>
    <div class="tab-content">
        <div class="active tab-pane" id="student">
          <div class="register-box-body">
            <p class="login-box-msg">Create a new account</p>
            <form ng-submit="onSubmit()">
              <div class="form-group has-feedback">
                <input type="text" class="form-control" ng-model="credentials.fullname" placeholder="Full name">
                <span class="fa fa-user-plus form-control-feedback"></span>
              </div>
              <div class="form-group has-feedback">
                <input type="text" ng-model="credentials.username" class="form-control" placeholder="Username">
                <span class="fa fa-user form-control-feedback"></span>
              </div>
              <div class="form-group has-feedback">
                <input type="email" class="form-control" ng-model="credentials.email" placeholder="Email">
                <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
              </div>
              <div class="form-group has-feedback">
                <input type="password" class="form-control" ng-model="credentials.password1" placeholder="Password">
                <span class="glyphicon glyphicon-lock form-control-feedback"></span>
              </div>
              <div class="form-group has-feedback">
                <input type="password" class="form-control" ng-model="credentials.password2" placeholder="Retype password">
                <span class="glyphicon glyphicon-log-in form-control-feedback"></span>
              </div> 
              <div class="form-group has-feedback">
                  <button  type="submit" class="btn btn-primary btn-block btn-flat">{{btn}}</button>
              </div>
            </form>

            <div class="social-auth-links text-center">
              <p>- OR -</p>
            </div>

            <a href="/student/login" class="text-center">I already have student account</a><br>
            <a href="/instructor/login" class="text-center">I already have instructor account</a>
          </div>
        </div>
    </div>
  </div>
  
  <!-- /.form-box -->
</div>
<!-- /.register-box -->


 <!-- angular  -->
    <script src="/lib/angular.min.js"></script>
    <script src="/lib/app.js"></script>
    <script src="/lib/angular-route.min.js"></script>
    <script src="/lib/loader.js"></script>
    <script src="/lib/dist/angular-animate.min.js"></script>
    <script src="/lib/dist/angular-sanitize.min.js"></script>
    <script src="/lib/dist/ng-file-upload.min.js"></script>
    <script src="/lib/dist/build/wysiwyg.min.js"></script>
    <script src="/lib/dist/SweetAlert.min.js"></script>
    <script src="/lib/dist/sweet-alert.min.js"></script>
    <script src="/lib/dist/ngToast.min.js"></script>
    <script src="/lib/student/register.controller.js"></script>
<!-- jQuery 3 -->
<script src="/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
</body>
</html>