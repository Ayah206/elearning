<% include header %>


<section class="content container">
          <div class="box box-solid">
            <div class="box-body">
              <div class="row">

                <div class="col-md-12" >
                  <div class="box box-solid">

                      <div class="box-header with-border">
                      <h3 class="box-title">Student 
                        <small><%= course.title %></small>
                      </h3>
                      <% include menu %>
                    </div>

 <!-- /.box-header -->
                      <div class="box-body">
                        <ul class="products-list product-list-in-box">
<%  for (var i = 0; i < course.participants.length; i++) { %>
                          <li class="item">
                            <div class="product-img">
                              <img src="/upload/avatar/<%= course.participants[i].avatar %> " class="img-circle" alt="Product Image">
                            </div>
                            <div class="product-info">
                        <a href="" class="product-title"> <%= course.participants[i].fullname %> (<%= course.participants[i].username %>)              
                              <span class="label label-info pull-right"><%= course.participants[i].department %></span></a>
                              <span class="product-description">
                                  <%= course.participants[i].email %>
                              </span>
                            </div>
                          </li>
<%}%>
                          <!-- /.item -->
                        </ul>
                      </div>
                      <!-- /.box-body -->
                 
                  <div class="row">

                    <div class="col-md-9" >
                         <div class="box-body">
                      <div class="box-group" id="accordion">
                        <!-- we are adding the .panel class so bootstrap.js collapse plugin detects it -->
                       
                      </div>
                    </div>
            <!-- /.box-body -->
                    </div>

                  </div>
                   


                     
                    
                  </div>
                </div>

               
              </div>


            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
</section>
<% include footer %>